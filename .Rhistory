n = length(x)
n2 = n %/% 2
sx = sort(x)
mx = median(x)
plot(mx - sx[1:n2], rev(sx)[1:n2] - mx,
xlab = "Distance Below Median",
ylab = "Distance Above Median")
abline(a = 0, b = 1, lty = "dotted")
}
