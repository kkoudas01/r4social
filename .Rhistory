pinakas_metabasisΒ <- function(dedomena, bima, arxi = NULL, telos = NULL) {
dedomenaM <- as.matrix(dedomena)
dedomenaM <- dedomenaM%^%bima
eyros <- c(arxi, telos)
corrplot(dedomenaM, is.corr = FALSE, method="color", col.lim = eyros, addCoef.col = 'black')
}
