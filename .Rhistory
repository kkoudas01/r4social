giniTotal0 <- gini[gini$AGE=="_T" & ((gini$TIME_PERIOD==2023 & gini$Gini_coefficient<=0.3) | gini$TIME_PERIOD<2023),]
giniTotal0Plot <- ggplot(giniTotal0, aes(x=TIME_PERIOD, y=Gini_coefficient, colour = REF_AREA))
